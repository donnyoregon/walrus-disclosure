+    z-index: 30;
+    pointer-events: none;
+}
+
+/* Arrow */
+.term .tooltip::after {
+    content: "";
+    position: absolute;
+    top: 100%;
+    left: 50%;
+    transform: translateX(-50%);
+    border-width: 6px;
+    border-style: solid;
+    border-color: var(--ifm-background-surface-color) transparent transparent transparent;
+}
+
+/* Show on hover/focus */
+.term:hover .tooltip,
+.term:focus-within .tooltip {
+    opacity: 1;
+    visibility: visible;
+    transform: translateX(-50%) translateY(-2px);
+}
+
+.tooltipTitle {
+    display: block;
+    font-weight: 600;
+    margin-bottom: 0.25rem;
+}
+
+.tooltipBody {
+    display: block;
+    font-size: 0.875rem;
+    line-height: 1.3;
+}
