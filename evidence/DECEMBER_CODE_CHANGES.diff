+
+import React from "react";
+import { useStats } from "react-instantsearch";
+
+export default function IndexStatsCollector({
+    indexName,
+    onUpdate,
+}: {
+    indexName: string;
+    onUpdate: (indexName: string, hits: number) => void;
+}) {
